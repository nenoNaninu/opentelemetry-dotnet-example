{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.AspNetCore.HttpLogging": "Information"
        }
    },
    "AllowedHosts": "*",
    "Kestrel": {
        "EndpointDefaults": {
            "Protocols": "Http2"
        },
        "Endpoints": {
            "E1": {
                "Url": "http://+:8080",
                "Protocols": "Http2"
            },
            "E2": {
                "Url": "http://+:8081",
                "Protocols": "Http1"
            }
        }
    }
}
